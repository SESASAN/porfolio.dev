---
import ExperienceItem from "./ExperienceItem.astro"
const EXPERIENCE=
[
{
        date: '05/05/2025 - 04/11/2025 (En curso)',
        title: 'Practicante de desarrollo de Software',
        description: 'Desempeñando tareas de desarrollo de software, para la automatización de procesos manuales, en la empresa Colombia Móvil.',
        link: '',
    },
]
---
<ol class="relative border-s border-gray-200 ml-2.5">
    {EXPERIENCE.map((experience) => (
        <li class="mb-10 ms-4">
            <ExperienceItem {...experience} />
        </li>
    ))}
</ol>